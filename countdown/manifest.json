{
    "manifest_version": 3,
    "name": "Sarradoro",
    "description": "Extension de gestion de temps",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "js": ["contentScript.js"],
        "css": ["contentScript.css"],
        "run_at": "document_idle",
        "matches": ["*://*/*", "file://*/*"]
      }
    ],
    "host_permissions": ["*://*/*",
    "file://*/*"]
  }